generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Post {
  id        String   @id @default(uuid())
  title     String
  text      String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) @updatedAt
}

model uploadimage {
  id  Int     @id @default(autoincrement())
  url String?
}

model upload {
  id  Int     @id @default(autoincrement())
  url String?
}
